<%= form_with model: @place do |form| %>
  <p>
    <%= form.label :name, "Place Name" %>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
    <%= form.label :date %>
    <%= form.text_field :date %>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </p>
  <%= form.submit %>
<% end %>